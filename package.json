{
  "name": "angular-nest-starter",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start:heroku": "node --optimize_for_size --max_old_space_size=460 dist/server",
    "postinstall": "npm run build:ssr",
    "dev": "concurrently \"nodemon \" \"ng serve --progress=false --proxy-config proxy.conf.json\"  ",
    "ng": "ng",
    "prestart": "npm run build",
    "start": "node dist/server/main.js",
    "build": "ng build --prod && tsc -p ./server",
    "build:universal": "npm run helper:build:client-and-server && npm run helper:webpack:nest",
    "serve:universal": "node ./dist/server.js",
    "helper:build:client-and-server": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "helper:webpack:nest": "webpack --config webpack.server.config.js --colors",
    "heroku-postbuild": "npm run build:unviersal",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.4.0",
    "@angular/animations": "^5.2.4",
    "@angular/cdk": "^5.2.4",
